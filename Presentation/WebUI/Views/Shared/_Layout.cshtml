<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="~/Themes/Default/img/favicon.png" type="image/png">
    <title>@ViewData["Title"] - Emrah Atalay</title>
    <link rel="stylesheet" href="~/Themes/Default/css/bootstrap.css">
    <link rel="stylesheet" href="~/Themes/Default/vendors/linericon/style.css">
    <link rel="stylesheet" href="~/Themes/Default/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Themes/Default/vendors/owl-carousel/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Themes/Default/vendors/lightbox/simpleLightbox.css">
    <link rel="stylesheet" href="~/Themes/Default/vendors/nice-select/css/nice-select.css">
    <link rel="stylesheet" href="~/Themes/Default/vendors/animate-css/animate.css">
    <link rel="stylesheet" href="~/Themes/Default/vendors/jquery-ui/jquery-ui.css">
    <!-- main css -->
    <link rel="stylesheet" href="~/Themes/Default/css/style.css">
    <link rel="stylesheet" href="~/Themes/Default/css/responsive.css">
</head>
<body>
    @await Component.InvokeAsync("Header")

    @RenderBody()

    @await Component.InvokeAsync("Footer")
    @await Component.InvokeAsync("Whatsapp")

    <script src="~/Themes/Default/js/jquery-3.2.1.min.js"></script>
    <script src="~/Themes/Default/js/popper.js"></script>
    <script src="~/Themes/Default/js/bootstrap.min.js"></script>
    <script src="~/Themes/Default/js/stellar.js"></script>
    <script src="~/Themes/Default/vendors/lightbox/simpleLightbox.min.js"></script>
    <script src="~/Themes/Default/vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="~/Themes/Default/vendors/isotope/imagesloaded.pkgd.min.js"></script>
    <script src="~/Themes/Default/vendors/isotope/isotope-min.js"></script>
    <script src="~/Themes/Default/vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Themes/Default/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/Themes/Default/vendors/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Themes/Default/vendors/flipclock/timer.js"></script>
    <script src="~/Themes/Default/vendors/counter-up/jquery.counterup.js"></script>
    <script src="~/Themes/Default/js/mail-script.js"></script>
    <script src="~/Themes/Default/js/theme.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
